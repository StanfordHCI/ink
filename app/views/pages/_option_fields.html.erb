<fieldset class="options, input-group-addon"> 
  <%= f.label :option_title, "Option title" %>
  <%= f.text_field :option_title, id: panel_id, :onblur=>'Tag(this.value, this.id)'%>
  <br/> 
  <%= f.label :info, "Option description"%>
  <%= f.text_area :info %>
  <br/>
  <%= f.label :photo, "Upload icon" %>
  <%= f.file_field :photo %>
  <%= f.hidden_field :_destroy %>
  <%= link_to "remove", "#", class: "remove_options" %>
</fieldset>
