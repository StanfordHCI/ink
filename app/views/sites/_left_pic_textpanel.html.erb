<body>
  <section id='parallax-quote' class='parallax'>
  <div class='parallax-background<%=panel.id %> parallax-background'></div>
  <div class='container'>
    <div class='row'>
      <div class='col-md-12'>
        <div class='parallax-content'>
          <table>
            <td style="width:50%">
              <%= image_tag panel.file %>
              <p><%= panel.caption %></p>
            </td>
            <td>
              <blockquote>
                <%= render 'sites/panel_title', panel: panel %>
                <footer>
                <%= render 'sites/panel_info', panel: panel %>
                </footer>
              </blockquote>
            </td>
          </table>
        </div>
      </div>
    </div>
  </div>
  </section>
</body>
