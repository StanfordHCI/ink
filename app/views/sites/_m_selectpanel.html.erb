<section id='page-about' class='page-about multi-select'>
<div class='container'>
  <header class='section-header'>
  <h2 class='section-title'>
    <%= render 'panel_title', panel: panel %>
  </h2>
  <div class='spacer'></div>
  <p class='section-subtitle'>
  <%= render 'panel_info', panel: panel %>
  </p>
  </header>
  <% for option in panel.options %>
    <div class='row'>
      <div class='col-md-4 text-center'>
        <div class='research hi-icon-effect-1'>
          <i class='hi-icon fa fa-<%option.id%>'></i>
        </div>
        <header class='process-header'>
        <h3>
          <%= option.option_title %>
        </h3>
        <p>
        "INSERT OPTION DESCRIPTION ATTRIBUTE HERE."
        </p>
        </header>
      </div>
    </div>
  <% end %>
</div>
</section>
